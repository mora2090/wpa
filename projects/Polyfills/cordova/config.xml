<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.sepidit" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MOrIw</name>
    <description>mori wpa</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://sepidit.ir/" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="default" />
    <preference name="Fullscreen" value="false" />
    <preference name="BackgroundColor" value="0x00000000" />
    <defaultlocale>English</defaultlocale>
    <allow-navigation hap-rule="yes" href="https://sepidit.ir/*" />
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
</widget>
